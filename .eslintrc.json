{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "FB": true
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/jsx-wrap-multilines": 0,
        "react/prop-types": 0,
        "jsx-a11y/href-no-hash": 0,
        "jsx-a11y/label-has-for": 0,
        "import/no-extraneous-dependencies": 0,
        "comma-dangle": 0,
        "arrow-parens": 0,
        "class-methods-use-this": 0,
        "consistent-return": 0
    }
}
